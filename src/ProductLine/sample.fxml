<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" prefHeight="497.0" prefWidth="673.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProductLine.ProductLineController">
  <children>
    <TabPane prefHeight="214.0" prefWidth="496.0" tabClosingPolicy="UNAVAILABLE">
      <tabs>
        <Tab fx:id="productLine" text="Product Line">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="259.0" prefWidth="388.0">
              <children>
                <GridPane layoutX="58.0" layoutY="41.0">
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                  <children>
                    <TextField fx:id="prodName" onInputMethodTextChanged="#handleAddProductButtonAction" GridPane.columnIndex="1" />
                    <TextField fx:id="manufacturer" onInputMethodTextChanged="#handleAddProductButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <TextField fx:id="itemType" onInputMethodTextChanged="#handleAddProductButtonAction" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    <Label prefHeight="17.0" prefWidth="79.0" text="Product Name" />
                    <Label prefHeight="17.0" prefWidth="82.0" text="Manufacturer" GridPane.rowIndex="1" />
                    <Label prefHeight="17.0" prefWidth="75.0" text="Item Type" GridPane.rowIndex="2" />
                  </children>
                </GridPane>
                <Button fx:id="addProd" layoutX="165.0" layoutY="138.0" mnemonicParsing="false" onAction="#handleAddProductButtonAction" stylesheets="@ProductLine.css" text="Add Product" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="produce" text="Produce">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="242.0" prefWidth="522.0">
                     <children>
                        <Label layoutX="53.0" layoutY="14.0" prefHeight="10.0" prefWidth="86.0" text="Choose Product" />
                        <ListView fx:id="catalog" layoutX="148.0" layoutY="6.0" prefHeight="177.0" prefWidth="198.0" />
                        <ComboBox fx:id="quantity" layoutX="346.0" layoutY="41.0" prefWidth="150.0" />
                        <Label layoutX="393.0" layoutY="14.0" text="Quantity" />
                        <Button fx:id="recordProd" layoutX="14.0" layoutY="107.0" mnemonicParsing="false" onAction="#handleRecordProdButtonAction" text="Record Production" />
                     </children></AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="prodLine" text="Production Log">
          <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="prodLog" layoutX="141.0" prefHeight="200.0" prefWidth="200.0" />
                     </children></AnchorPane>
          </content>
        </Tab>
      </tabs>
    </TabPane>
      <TableView prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
        <columns>
          <TableColumn fx:id="existingProducts" prefWidth="495.0" text="Existing Products" />
        </columns>
      </TableView>
  </children>
  <columnConstraints>
    <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="230.0" minHeight="90.0" prefHeight="214.0" />
      <RowConstraints maxHeight="140.0" minHeight="0.0" prefHeight="46.0" />
      <RowConstraints maxHeight="25.0" minHeight="0.0" prefHeight="0.0" />
  </rowConstraints>

</GridPane>
