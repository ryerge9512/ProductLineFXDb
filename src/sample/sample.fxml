      <TabPane prefHeight="200.0" prefWidth="316.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="productLine" text="Product Line">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <GridPane layoutX="58.0" layoutY="41.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <TextField GridPane.columnIndex="1" />
                              <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
                              <TextField GridPane.columnIndex="1" GridPane.rowIndex="2" />
                              <Label prefHeight="17.0" prefWidth="79.0" text="Product Name" />
                              <Label prefHeight="17.0" prefWidth="82.0" text="Manufacturer" GridPane.rowIndex="1" />
                              <Label prefHeight="17.0" prefWidth="75.0" text="Item Type" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                        <Button layoutX="165.0" layoutY="138.0" mnemonicParsing="false" text="Add Product" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="recordProd" text="Record Production">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab fx:id="prodLine" text="Production Log">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
